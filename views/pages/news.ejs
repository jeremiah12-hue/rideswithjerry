            <div class="news_top_nav_icon"><i class="fa-brands fa-linkedin-in"></i></div>
        </div>
    
        <div class="news_tag_container">
            <div class="news_tag_item">
                <p>({% NEWS_TAG %})</p>
                <p>({% NEWS_BRAND %})</p>
            </div>
        </div>
    
        <div class="news_head">
            <h1>({% NEWS_HEADER %})</h1>
            <h2>({% NEWS_INTRO %})</h2>
        </div>
    
        <div class="news_main">

            <div class="img">
                <img loading="lazy" src="({% NEWS_IMG %})" alt="Update Image!" />
            </div>
    
            <div class="news_news">
                <p>
                    ({% NEWS %})
                </p>
            </div>
    
            <div class="date">
                <p>({% NEWS_DATE %})</p>
            </div>
        </div>
    
        <hr>
        <hr>
    
        <div class="news_review">
    
            <div class="review_icons">
                <i class="fa-solid fa-face-sad-tear fa-beat-fade" style="color: #eb0000;"></i>
                <i class="fa-solid fa-face-tired fa-beat-fade" style="color: #FFD43B;"></i>
                <i class="fa-solid fa-face-rolling-eyes fa-beat-fade" style="color: #434137;"></i>
                <i class="fa-solid fa-face-surprise fa-beat-fade" style="color: #005eff;"></i>
                <i class="fa-solid fa-face-smile-wink fa-beat-fade" style="color: #09f005;"></i>
            </div>
    
            <form action="" method="post">
    
                <textarea name="thoughts" id="wide_review" placeholder="Review ...!" required ></textarea>
    
                <button type="submit" id="wideReviewSubmit">Submit</button>
            
            </form>
        </div>
    
        <hr>
        <hr>
    
        <div class="loadMore_container">
            <div class="loadMore_item">
                <p>Also Enjoy!</p>
            </div>
        </div>
    

        
    </section>

