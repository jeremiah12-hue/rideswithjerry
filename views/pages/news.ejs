    <section class="wide_main_news">

        <div class="news_top_nav">
            <div class="news_top_nav_icon"><i class="fa-brands fa-whatsapp"></i></div>
            <div class="news_top_nav_icon"><i class="fa-brands fa-facebook-f"></i></div>
            <div class="news_top_nav_icon"><i class="fa-brands fa-instagram"></i></div>
